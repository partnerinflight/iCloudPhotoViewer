    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Attach to PhotoFrame",
            "type": "python",
            "request": "attach",
            "localRoot": "${workspaceRoot}",
            "remoteRoot": "/home/pi/iCloudPhotoViewer",
            "port": 5678,
            "secret": "my_secret",
            "host": "192.168.1.6",
            "preLaunchTask": "remote-deploy-debug"
        }
    ]
}